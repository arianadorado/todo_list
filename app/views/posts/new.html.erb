<h1>Posts#new</h1>
<p>Find me in app/views/posts/new.html.erb</p>

<h1> New Post </h1>
<%= form_for @post do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %>

  <%= f.label :catagory %>
  <%= f.text_field :catagory %>

  <%= f.label :priority %>
  <%= f.text_field :priority %>

  <%= f.submit %>
<% end %>