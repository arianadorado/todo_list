<h1> Edit Post </h1>
<%= form_for @post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :catagory %>
  <%= f.text_field :catagory %>

  <%= f.label :priority %>
  <%= f.select :priority, %w{High Medium Low}, :prompt => 'Select', required: true, placeholder: 'priority', class: 'form-control' %>

  <%= f.label :completed %>
  <%= f.check_box :completed %>

  <%= f.submit %>
<% end %>